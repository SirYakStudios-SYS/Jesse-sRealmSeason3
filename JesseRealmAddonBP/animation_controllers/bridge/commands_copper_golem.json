{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.bridge_copper_golem.execute_commands": {
			"states": {
				"default": {
					"transitions": [
						{
							"execute_command_id_2": "query.skin_id == 2"
						},
						{
							"execute_command_id_1": "query.skin_id == 1"
						}
					]
				},
				"execute_command_id_1": {
					"transitions": [
						{
							"default": "query.skin_id != 1"
						}
					],
					"on_entry": [
						"particle ~ ~ ~ minecraft:electric_spark_particle",
						"@s bridge:remove_command_id_1"
					]
				},
				"execute_command_id_2": {
					"transitions": [
						{
							"default": "query.skin_id != 2"
						}
					],
					"on_entry": [
						"/playanimation @s animation.copper_golem.rotate_head",
						"@s bridge:remove_command_id_2"
					]
				}
			}
		}
	}
}